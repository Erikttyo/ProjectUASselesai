<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.prasettyo.ujungtombak">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Pilihan"/>
        <activity android:name=".Pesanan"
            android:label="Pesanan"/>
        <activity android:name=".Credit"
            android:label="Credit"/>
        <activity android:name=".Detail"
            android:label="Detail"/>
        <activity android:name=".Laporan"
            android:label="Laporan"/>
        <activity android:name=".LaporanRinci"
            android:label="Laporan Rinci"/>
    </application>

</manifest>